<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.frabbi.meem">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name=".MainActivity"
            android:theme="@style/newTheme" />
        <activity
            android:name=".ActivityLogin"
            android:theme="@style/newTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingsActivity"  android:label="Settings"/>
        <activity android:name=".BottomBarActivity" />
        <activity android:name=".CircleActivity" android:label="Circle Creation"/>
        <activity android:name=".CheckInActivity" android:label="Check In"/>
        <activity android:name=".MapActivity" android:label="Map"></activity>
        <activity android:name=".ActivityAccount" android:label="Account"></activity>
    </application>

</manifest> <!-- android:label="@string/app_name" -->
